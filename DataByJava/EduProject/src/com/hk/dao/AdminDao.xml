<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hk.dao.AdminDao">
	
	
	<select id="checkAdmin" resultType="Admin">
		select * from admin 
		 
		where admin_user = #{0} and admin_pwd= #{1}
		
	</select>
	<select id="selectAdmin" resultType="Admin">
		select * from admin 
		
	</select>

</mapper>